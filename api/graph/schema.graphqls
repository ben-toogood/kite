# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type User {
  id: ID!
  firstName: String!
  lastName: String!
}

type Comment {
  Author: User!
  Message: String!
}

type Post {
  Author: User!
  Comments: [Comment]
}

input Signup {
  firstName: String!
  lastName: String!
  email: String!
}

type Mutation {
  signup(input: Signup!): User!
}
